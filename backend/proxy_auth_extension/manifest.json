{
  "version": "1.0.0",
  "manifest_version": 2,
  "name": "proxy_auth_extension",
  "permissions": [
  "proxy",
  "tabs",
  "unlimitedStorage",
  "storage",
  "<all_urls>",
  "webRequest",
  "webRequestBlocking"
  ],
  "background": {
  "scripts": ["background.js"]
  },
  "minimum_chrome_version":"20.0.0"
  }